{
  "name": "storefrontbackend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/server.ts",
    "build": "npx tsc",
    "dev": "npx ts-node-dev --respawn --transpile-only ./src/server.ts",
    "create-db": "ENV=dev db-migrate db:create storefront_backend",
    "deploy": "eb deploy",
    "delete-db-test": "db-migrate db:drop storefront_backend_test",
    "jasmine": "jasmine",
    "test": "db-migrate db:create storefront_backend_test && db-migrate --env test up && npm run build && ENV=test npm run jasmine && db-migrate db:drop storefront_backend_test"
  },
  "author": "nguyen huu hung",
