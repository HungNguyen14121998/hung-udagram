    "@types/node": "^22.7.0",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jasmine": "^5.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
