{
    "scripts": {
        "frontend:install": "cd udagram/udagram-frontend && npm install",
        "frontend:start": "cd udagram/udagram-frontend && npm run start",
        "frontend:build": "cd udagram/udagram-frontend && npm run build",
        "frontend:deploy": "cd udagram/udagram-frontend && npm run deploy",
        "api:install": "cd udagram/back-end && npm install .",
        "api:build": "cd udagram/back-end && npm run build",
        "api:start": "cd udagram/back-end && npm run dev",
        "api:deploy": "cd udagram/back-end && npm run deploy",
        "deploy": "npm run api:deploy && npm run frontend:deploy"
    }
}